  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "@babel/core": "^7.11.6",
    "bluebird": "^3.7.2",
    "gatsby": "^2.24.53",
    "gatsby-plugin-feed": "^2.5.11",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-layout": "^1.3.10",
    "gatsby-plugin-offline": "^3.2.26",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.6.31",
    "gatsby-remark-copy-linked-files": "^2.3.13",
    "gatsby-remark-images": "^3.3.28",
    "gatsby-remark-prismjs": "^3.5.11",
    "gatsby-remark-responsive-iframe": "^2.4.12",
    "gatsby-remark-smartypants": "^2.3.10",
    "gatsby-source-filesystem": "^2.3.27",
    "gatsby-transformer-json": "^2.4.11",
    "gatsby-transformer-remark": "^2.8.32",
    "gatsby-transformer-sharp": "^2.5.14",
    "history": "^4.7.2",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.21.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0",
    "react-transition-group": "^2.5.0",
    "three": "^0.98.0"
  },
  "devDependencies": {
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "gh-pages": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.2",
    "prettier": "^2.1.1"
  },
  "scripts": {
    "build": "npm-run-all -s validate gatsby:build",
    "dev": "npm-run-all -p prettier:watch eslint:watch gatsby:develop",
    "validate": "npm-run-all -s prettier eslint",
    "deploy": "npm-run-all -s clean build ghpages",
    "gatsby:build": "gatsby build",
    "gatsby:develop": "gatsby develop --port 9000",
    "prettier": "prettier --write \"{,!(.cache|public)/**/}*.{js,json,jsx,md}\"",
    "prettier:watch": "onchange \"{,!(.cache|public)/**/}*.{js,json,jsx,md}\" -- prettier --write {{changed}}",
    "eslint": "eslint --fix --ext .js,.jsx .",
    "eslint:watch": "onchange \"{,!(.cache|public)/**/}*.{js,jsx}\" -- eslint --fix {{changed}}",
    "ghpages": "gh-pages --dist public --branch master --repo https://github.com/nopjia/nopjia.github.io",
    "clean": "rm -rf .cache public"
  }
}
