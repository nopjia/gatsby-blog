    "npm-run-all": "^4.1.3",
    "onchange": "^4.1.0",
    "prettier": "^1.12.0"
  },
  "scripts": {
    "build": "npm-run-all -s prettier eslint gatsby:build",
    "dev": "npm-run-all -p gatsby:develop prettier:watch eslint:watch",
    "gatsby:build": "gatsby build",
    "gatsby:develop": "gatsby develop",
    "prettier": "prettier --write \"{,!(.cache|public)/**/}*.{js,json,jsx,md}\"",
    "prettier:watch": "onchange \"{,!(.cache|public)/**/}*.{js,json,jsx,md}\" -- prettier --write {{changed}}",
    "eslint": "eslint --ext .js,.jsx .",
    "eslint:fix": "eslint --ext .js,.jsx .",
    "eslint:watch": "onchange \"{,!(.cache|public)/**/}*.{js,jsx}\" -- eslint {{changed}}"
  }
}
